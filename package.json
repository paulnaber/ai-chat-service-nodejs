{
  "name": "ai-chat-service-nodejs",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "node build/src/index.js",
    "start:test": "node -r dotenv/config build/src/index.js dotenv_config_path=.env.test",
    "docker-compose:test": "docker compose --env-file .env.test up",
    "dev": "npm run tsoa && npm run tsc && node build/src/index.js",
    "dev:logs": "npm run dev | tee server.log",
    "watch": "npx tsc-watch --onSuccess \"node dist/index.js\" | tee server.log",
    "generate": "drizzle-kit generate",
    "migrate": "drizzle-kit migrate",
    "test": "jest",
    "tsoa": "tsoa spec-and-routes",
    "tsc": "tsc --outDir build --experimentalDecorators"
  },
  "author": "Paul Naber",
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.13.8",
    "@types/supertest": "^6.0.2",
    "@types/swagger-ui-express": "^4.1.8",
    "cors": "^2.8.5",
    "drizzle-kit": "^0.30.5",
    "jest": "^29.7.0",
    "jwks-rsa": "^3.1.0",
    "supertest": "^7.1.0",
    "swagger-ui-express": "^5.0.0",
    "ts-jest": "^29.2.6",
    "tsoa": "6.6.0",
    "typescript": "^5.8.2",
    "vitest": "^3.0.7"
  },
  "dependencies": {
    "@tsoa/runtime": "^6.6.0",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.4.7",
    "drizzle-orm": "^0.40.0",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "postgres": "^3.4.5",
    "tsc-watch": "^6.0.4",
    "vite": "^5.4.9"
  }
}
